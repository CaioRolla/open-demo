  "scripts": {
    "------SHARED------": "------------------------",
    "ng": "nx",
    "nx": "nx",
    "i18n-all": "concurrently -kill-others \"npm run i18n-adv-app\" \"npm run i18n-adv-portal-app\" \"npm run i18n-wish-app\" \"npm run i18n-cron-app\" ",
    "xliffmerge": "xliffmerge",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "typeorm": "typeorm-ts-node-commonjs",
    "------WISH------": "------------------------",
    "build-wish-server": "ng build --project wish-server --configuration=production",
    "build-wish-app": "ng server --project wish-app --configuration=production",
    "serve-wish-app": "ng serve-ssr --project wish-app",
    "serve-wish-server": ". ./apps/wish-server/local.sh && ng serve --project wish-server",
    "i18n-wish-app": "ng extract-i18n --project=wish-app --outputPath ./apps/wish-app/locale && npx xliffmerge --profile apps/wish-app/xliffmerge.json",
    "wish-server:typeorm:cli": "export NODE_ENV=migration &&. ./apps/wish-server/local.sh && ts-node --project apps/wish-server/tsconfig.app.json -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./apps/wish-server/src/ormconfig.ts",
    "wish-server:typeorm:cli:ci": "export NODE_ENV=migration && ts-node --project apps/wish-server/tsconfig.app.json -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./apps/wish-server/src/ormconfig.ts",
    "wish-server:migration:generate": "npm run wish-server:typeorm:cli -- migration:generate -n",
    "wish-server:migration:create": "npm run wish-server:typeorm:cli -- migration:create -n",
    "wish-server:migration:run": "npm run wish-server:typeorm:cli -- migration:run",
    "wish-server:migration:show": "npm run wish-server:typeorm:cli -- migration:show",
    "wish-server:migration:revert": "npm run wish-server:typeorm:cli -- migration:revert",
    "wish-server:migration:run:ci": "npm run wish-server:typeorm:cli:ci -- migration:run",
    "wish-server:migration:show:ci": "npm run wish-server:typeorm:cli:ci -- migration:show",
    "wish-server:migration:revert:ci": "npm run wish-server:typeorm:cli:ci -- migration:revert",
    "serve-wish": "concurrently --kill-others \"npm run serve-wish-server\" \"npm run serve-wish-app\""
  },
